RULENAME: nphrase
#to exclude "no da" constructions###
 FOCUS: ptl:snr|no=SNR^v:cop|* 
 POINTS: 

#### N+no+(N)  F2
# Papa no (kutsu)  F2
 FOCUS: ptl:attr|no=GEN
 POINTS: F2 

#### A+N  F2
# oishikunai gohan  F2
 FOCUS: adj|*-NEG-PRES*^n*|*
 POINTS: F2 C3
# oishii gohan  F2
 FOCUS: adj|*-PRES*^n*|*
 POINTS: F2 C1
# okkii gohan  F2
 FOCUS: adj:*|*-PRES*^n*|*
 POINTS: F2 C1
# oishikatta gohan F2
 FOCUS: adj|*-PAST*^n*|*
 POINTS: F2 C1

#### N to N  F3
# gohan to raisu   F3
 FOCUS: ptl:coo|to=and
 POINTS: F3

#### adn + ^n*|    F3 
# ookina hito ; konna hito  F3
 FOCUS: adn|*^n*|*+adn|*^ptl:fina|ne*^n*|*+adn*|*^n*|*+adn*|*^ptl:fina|ne*^n*|*
 POINTS: F3

#### cop + ^n*|  F3
# janai hito  D3 F3
 FOCUS: v:cop|de&wa-NEG-PRES=be^n*|*
 POINTS: D3 F3

# datta hito  D4 F3
 FOCUS: v:cop|da-PAST=be^n*|*
 POINTS: D4 F3

